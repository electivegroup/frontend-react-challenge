{
  "name": "electivegroup",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "lodash": "4.17.11",
    "react": "*",
    "react-dom": "*",
    "react-redux": "5.0.7",
    "react-scripts": "2.1.8",
    "react-trello": "2.1.3",
    "redux": "4.0.0",
    "redux-actions": "2.6.1",
